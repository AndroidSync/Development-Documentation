#%RAML 1.0 DataType

type: object
properties:
  data:
    type: object
    properties:
      time:
        type: integer
        description: "A unix timestamp of time taken when clipboard is copied"
      hash:
        type: object
        properties:
          crc32:
            type: string
            description: "An CRC32 checksum of the data payload"
      data:
        type: string
        description: "Fully processed clipboard data (Compressed, Encrypted, Base64'd)"
