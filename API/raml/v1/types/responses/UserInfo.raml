#%RAML 1.0 DataType

type: object
properties:
  success: !include Success.raml
  error: !include Error.raml
  data:
    type: object
    properties:
      register-date:
        type: integer
        description: "A unix timestamp of registration date"
      clip-count:
        type: integer
        description: "Count how many clips has the user made"