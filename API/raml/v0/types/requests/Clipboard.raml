#%RAML 1.0 DataType

type: object
properties:
  data:
    type: object
    properties:
      timestamp:
        type: integer
        description: "A unix timestamp of time taken when clipboard is copied in milliseconds"
      payload: !include ../Payload.raml
      payload-type:
        enum: ["TEXT", "IMAGE", "VIDEO", "BINARY"]
        description: "The data-type of the payload"
